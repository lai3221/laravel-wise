{
    "name": "lai3221/laravel-wise",
    "description": "A Laravel package for integrating with the Wise (formerly TransferWise) API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "lai3221",
            "email": "lai3221@163.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/http": "^11.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Lai3221\\LaravelWise\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lai3221\\LaravelWise\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lai3221\\LaravelWise\\Providers\\WiseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
